{
  "manifest_version": 2,
  "name": "PageZipper",
  "version": "1.5",
  "description": "Painlessly merge a series of web pages into one",
  "homepage_url": "http://printwhatyoulike.com/pagezipper",
  "options_page": "extension_options/options.html",
  "icons": { "16": "zipper_16.png",
             "48": "zipper_48.png",
             "128": "zipper_128.png" },
  "background": {
    "scripts": ["extension_options/common.js", "background.js", "extension_common/background-common.js"]
  },
  "browser_action": {
      "name": "PageZipper",
      "default_title": "PageZipper",
      "default_icon": {
          "19": "icon19.png",
          "38": "icon38.png"
        }
  },
  "web_accessible_resources": ["*"],
  "permissions": [
    "tabs",
    "http://*/*", "https://*/*",
    "activeTab",
    "storage",
    "webNavigation"
  ]
}